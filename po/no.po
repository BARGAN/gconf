msgstr "Ignorerer XML-node med navn `%s': %s"

#: backends/xml-entry.c:213 gconf/gconf-internals.c:2159
#, c-format
msgid "%s"
msgstr "%s"

#: backends/xml-entry.c:340
#, c-format
msgid "Ignoring schema name `%s', invalid: %s"
msgstr "Ignorerer schema-navn `%s', ugyldig: %s"

