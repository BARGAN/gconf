msgid "Failed to shut down backend"
msgstr "Nisem uspel ustaviti ozadnega modula"

#: gconf/gconf-client.c:342 gconf/gconf-client.c:360
#, c-format
msgid "GConf Error: %s\n"
msgstr "Napaka gconf %s\n"

