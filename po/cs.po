msgstr "Chyba při ukládání stromu GConf do '%s': %s\n"

#: backends/gconf-merge-tree.c:121
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Použití: %s <adresář>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Použití: %s <adresář>\n"
"  Sloučí hierarchii systému souborů modulu markup tvaru:\n"
"    adr/%%gconf.xml\n"
"        podadr1/%%gconf.xml\n"
"        podadr2/%%gconf.xml\n"
"  to:\n"
"    adr/%%gconf-tree.xml\n"

