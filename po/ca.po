msgstr "Entrada sense nom al fitxer XML `%s', s'ignora"

#: backends/xml-dir.c:979
#, c-format
msgid "Toplevel node in XML file `%s' is not an <entry>, ignoring"
msgstr "El node d'alt nivell al fitxer XML `%s' no és una <entry>, s'ignora"

#: backends/xml-dir.c:1067
#, c-format
msgid "Failed to create file `%s': %s"
msgstr "No s'ha pogut crear el fitxer `%s': %s"

#: backends/xml-dir.c:1075 gconf/gconf-internals.c:2472
#, c-format
msgid "Failed to close file `%s': %s"
msgstr "No s'ha pogut tancar el fitxer `%s': %s"

#. There was an error
#: backends/xml-entry.c:149
#, c-format
msgid "Ignoring XML node with name `%s': %s"
msgstr "S'ignora el node XML anomenat `%s': %s"

#: backends/xml-entry.c:200
#, c-format
msgid "%s"
msgstr "%s"

#: backends/xml-entry.c:327
#, c-format
msgid "Ignoring schema name `%s', invalid: %s"
msgstr "S'ignora l'esquema anomenat `%s', no és vàlid: %s"

#. FIXME for nodes with no value stored, but containing a schema name,
#. * we improperly log an error here
#.
#: backends/xml-entry.c:373
#, c-format
msgid "Ignoring XML node `%s', except for possible schema name: %s"
msgstr "S'ignora el node XML `%s', excepte per a un possible nom d'esquema: %s"

#: backends/xml-entry.c:724
#, c-format
msgid "Failed reading default value for schema: %s"
msgstr "No s'ha pogut llegir el valor per defecte de l'esquema: %s"

#: backends/xml-entry.c:938
#, c-format
msgid "No \"type\" attribute for <%s> node"
msgstr "No hi ha atribut \"type\" al node <%s>"

#: backends/xml-entry.c:952
#, c-format
msgid "A node has unknown \"type\" attribute `%s', ignoring"
msgstr "Un node té un atribut \"type\" desconegut `%s', s'ignora"

#: backends/xml-entry.c:967
msgid "No \"value\" attribute for node"
msgstr "No hi ha atribut \"value\" al node"

#: backends/xml-entry.c:1015 backends/xml-entry.c:1091
#, c-format
msgid "Didn't understand XML node <%s> inside an XML list node"
msgstr "No s'entén el node XML <%s> dins del node de llista XML"

#: backends/xml-entry.c:1049
msgid "Invalid type (list, pair, or unknown) in a list node"
msgstr "Tipus incorrecte (llista, parella o desconegut) en un node de llista"

#: backends/xml-entry.c:1072
#, c-format
msgid "Bad XML node: %s"
msgstr "Node XML erroni: %s"

#: backends/xml-entry.c:1080
#, c-format
msgid "List contains a badly-typed node (%s, should be %s)"
msgstr "La llista conté un node de tipus incorrecte (%s, hauria de ser %s)"

#: backends/xml-entry.c:1132
#, c-format
msgid "Ignoring bad car from XML pair: %s"
msgstr "S'ignora el caràcter erroni de la parella XML: %s"

#: backends/xml-entry.c:1141 backends/xml-entry.c:1164
msgid "parsing XML file: lists and pairs may not be placed inside a pair"
msgstr ""
"S'analitza el fitxer XML: les llistes i les parelles no es poden posardins "
"una parella"

#: backends/xml-entry.c:1154
#, c-format
msgid "Ignoring bad cdr from XML pair: %s"
msgstr "S'ignora el cdr erroni de la parella XML: %s"

#: backends/xml-entry.c:1173
#, c-format
msgid "Didn't understand XML node <%s> inside an XML pair node"
msgstr "No s'entén el node XML <%s> del node de parella XML"

#: backends/xml-entry.c:1191
msgid "Didn't find car and cdr for XML pair node"
msgstr "No es troba el caràcter i el cdr del node de parella XML"

#: backends/xml-entry.c:1197
msgid "Missing cdr from pair of values in XML file"
msgstr "S'ha perdut el cdr de la parella de valors al fitxer XML"

#: backends/xml-entry.c:1204
msgid "Missing car from pair of values in XML file"
msgstr "S'han perdut els caràcters de la parella de valors al fitxer XML"

#: backends/xml-entry.c:1209
msgid "Missing both car and cdr values from pair in XML file"
msgstr ""
"S'han perdut els valors de cdr i de caràcters de la parella al fitxer XML"

