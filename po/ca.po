#, c-format
msgid "Failed to stat `%s': %s"
msgstr "No s'ha pogut iniciar `%s': %s"

#: backends/xml-dir.c:950
#, c-format
msgid "Duplicate entry `%s' in `%s', ignoring"
msgstr "Entrada duplicada `%s' a `%s', s'ignora"

