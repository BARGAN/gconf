#, c-format
msgid "Error removing dir `%s': %s"
msgstr "Virhe hakemistoa '%s' poistaessa: %s"

#: gconf/gconf-database.c:1374
#, c-format
msgid "Failed to get all entries in `%s': %s"
msgstr "Kaikkien hakemiston '%s' merkintöjen saaminen epäonnistui: %s"

