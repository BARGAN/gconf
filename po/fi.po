msgstr "Tulkkausvirhe"

#: gconf/gconf-error.c:32
msgid "Corrupt data in configuration source database"
msgstr "Asetuslähteen tietokannassa on viallista dataa."

#: gconf/gconf-error.c:33
msgid "Type mismatch"
msgstr "Tyyppivirhe"

#: gconf/gconf-error.c:34
