#, c-format
msgid "Adding source `%s'\n"
msgstr "Lisätään lähde '%s'\n"

#: gconf/gconf-internals.c:880
#, c-format
msgid "Read error on file `%s': %s\n"
msgstr "Virhe luettaessa tiedostoa '%s': %s\n"

#: gconf/gconf-internals.c:1322
#, c-format
msgid "Expected list, got %s"
