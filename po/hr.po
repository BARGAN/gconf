#, c-format
msgid "No such file `%s'\n"
msgstr "Nema datoteke `%s'\n"

#: gconf/gconf-backend.c:195
#, c-format
msgid "Bad address `%s'"
msgstr "Loša adresa `%s'"

#: gconf/gconf-backend.c:220
